{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "no-restricted-imports": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "curly": "error"
  }
}
